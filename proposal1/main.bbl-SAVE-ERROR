      \strng{namehash}{8dd65d8c4825ea776954c3ebed9468ee}
      \strng{fullhash}{2f901bdd18bd02af5a741c8765270748}
      \strng{fullhashraw}{2f901bdd18bd02af5a741c8765270748}
