    \entry{shakeriNaturalLanguageProcessing2025a}{article}{}{}
      \name{author}{2}{}{%
        {{hash=0e756bc49616642232f3597e08222f83}{%
           family={Shakeri},
           familyi={S\bibinitperiod},
           given={Arezo},
